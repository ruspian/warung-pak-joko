#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/pian/Documents/e-commerce/backend/node_modules/mkdirp/bin/node_modules:/Users/pian/Documents/e-commerce/backend/node_modules/mkdirp/node_modules:/Users/pian/Documents/e-commerce/backend/node_modules:/Users/pian/Documents/e-commerce/node_modules:/Users/pian/Documents/node_modules:/Users/pian/node_modules:/Users/node_modules:/node_modules:/Users/pian/Documents/e-commerce/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/pian/Documents/e-commerce/backend/node_modules/mkdirp/bin/node_modules:/Users/pian/Documents/e-commerce/backend/node_modules/mkdirp/node_modules:/Users/pian/Documents/e-commerce/backend/node_modules:/Users/pian/Documents/e-commerce/node_modules:/Users/pian/Documents/node_modules:/Users/pian/node_modules:/Users/node_modules:/node_modules:/Users/pian/Documents/e-commerce/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mkdirp/bin/cmd.js" "$@"
else
  exec node  "$basedir/../mkdirp/bin/cmd.js" "$@"
fi
